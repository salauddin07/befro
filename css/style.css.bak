/* ========== GOOGLE FONT ==========  */
@import url('https://fonts.googleapis.com/css2?family=Poppins:wght@400;500;600;700;800&display=swap');

/* ==========  RESET & HELPERS ==========  */
* {
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}

body {
    font-family: "Poppins", sans-serif;
    color: #777;
    line-height: 1.8;
    font-size: 16px;
}

h1,
h2,
h3,
h4,
h5,
h6 {
    font-weight: 700;
    color: #222;
    line-height: 1.2;
    margin-bottom: 10px;
}

.h1 {
    font-size: 54px !important;
}

.h2 {
    font-size: 36px;
}

.h3 {
    font-size: 28px;
}

.h4 {
    font-size: 24px;
}

.h5 {
    font-size: 18px;
}

@media (max-width: 600px) {
    .h1 {
        font-size: 38px;
    }

    .h2 {
        font-size: 32px;
    }

    .h3 {
        font-size: 24px;
    }

    .h4 {
        font-size: 20px;
    }
}

.slide {
    text-align: center;
    transform: translateY(100%);
    /* margin-top: 100px; */
}

.navbar{
	position:relative;
}
nav.navbar.navbar-expand-lg.navbar-light {

	position:absolute;
	top:0;
	z-index:999;
	padding:0 30px;
}
li.nav-item a {
    color: #fff !important;
    font-size: 15px;
    font-weight: 500;
}
.hero{
	height:100vh;
	background-image: url(../images/cover_1.jpg);
	background-size:cover;
	background-position:center;
	background-repeat:no-repeat;
}
.slide h1 {
    color: #fff;
    text-align: center;
	font-size:54px;
	margin-bottom: 25px;
	
}
.slide p {
	color:#fff;
    text-align: center;
    width: 685px;
    margin: auto;
}
a.btn {
    background-color: #b3040d;
    color: #fff;
    font-weight: 700;
    padding: 12px 24px;
    border: 2px solid #b3040d;
    margin: 4px;
	margin-top:20px;
	transition:.5s;
	outline:none !important;
	box-shadow: 0 4px 20px rgb(0 0 0 / 50%);
}
a.btn:hover{
	 color: #fff;
    transform: translateY(3px);
}
a.btn.work {
	background-color: transparent;
    color: #fff;
    font-weight: 700;
    padding: 12px 24px;
    border: 2px solid #fff;
    margin: 4px;
	margin-top:20px;
	transition:.5s;
}
a.btn.work:hover{
	background:#fff;
	color:#000;
}
a.btn.working:hover {
   box-shadow:none;
}








.brand-img img {
    width: 100%;
	border-radius:5px;
}
section.brands {
    padding: 130px 0 40px 0px;
}
.brand-box {
    margin-top: -25px;
}
.brand-box h2 {
    font-size: 35px;
}
.brand-box p {
    font-size: 15px;
    color: #7a6a6a;
}
a.btn.working {
	outline:none;
}







section.service {
    padding: 130px 0 30px 0;
}
h2.h2 {
    text-align: center;
    font-weight: 800;
    font-size: 40px;
}
p.para {
    text-align: center;
    max-width: 650px;
    margin: auto;
}
.row.toppar {
    margin-top: -18px;
}
.service-box {
    text-align: center;
    margin-top: 60px;
	transition:  0.4s;
	padding:20px;
	border-bottom: 2px solid #b3040d;
	border-left: 2px solid #b3040d;
	border-right: 2px solid #b3040d;
	border-top: 2px solid #b3040d;
	
	
}
.service-box i {
    font-size: 50px;
    color: #b3040d;
	position:relative;
}

.service-box i::after {
    content: " ";
    width: 40px;
    height: 40px;
    background: linear-gradient(rgba(179, 4, 13, 0.7), rgba(179, 4, 13, 0.184));
    /* display: block; */
    position: absolute;
    top: 24px;
    transform: translateX(-30px);
}
.service-box h4 {
    font-weight: 800;
    margin-top: 20px;
    margin-bottom: 20px;
}
.service-box p {
    font-size: 15px;
    color: #887878;
}
.service-box:hover{
	
	box-shadow:0 4px 20px rgba(0,0,0,0.4);
	cursor:pointer;
	
	
}





/* ==========  PORTFOLIO ==========  */
section.portfolio {
    padding: 100px 0 30px 0;
	text-align: center;
	
	
	
}
.portfolio-img img {
    
    width: 100%;
    border-radius: 2px;
}
.portfolio-img{
	position:relative;
	overflow:hidden;
	margin-top:50px;
}
.portfolio-img .overlay {
   
  
    position: absolute;
    top: 0%;
    left: -100%;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.8);
    border-radius: 5px;
    transition: all 0.4s ease;
    opacity: 0;

}
.portfolio-img:hover .overlay{
	opacity:1;
	
	left:0;
	
}
.portfolio-img .web{
	position:absolute;
	top:60%;
	left:50%;
	transform:translate(-50%,-50%);
	opacity:0;
	transition:all 0.4s ease;
	color:#fff !important;
}
.portfolio-img:hover .web{
	opacity:1;
	top:50%;
}
.web h3 {
    color: #fff !important;
    font-size: 20px;
}
.web h5 {
    color: #fff !important;
    
}




















